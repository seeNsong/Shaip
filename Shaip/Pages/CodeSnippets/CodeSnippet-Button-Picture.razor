<CodeSnippetTabbed>
    <CodeSnippetTabPage Text="Razor">@(@"<DxButton RenderStyle=""ButtonRenderStyle.Dark"" Text=""Undo"" Title=""Undo the last action."" IconCssClass=""undo"" style=""margin-right: 0.5em;""/>
<DxButton RenderStyle=""ButtonRenderStyle.Dark"" Text=""Redo"" Title=""Restore the previously undone action."" IconCssClass=""redo"" IconPosition=""ButtonIconPosition.AfterText"" />")</CodeSnippetTabPage>
    <CodeSnippetTabPage Text="CSS">@(@".undo {
    width: 16px;
    height: 16px;
    background-image: url(""../images/undo.svg"");
    margin: 0 8px 0 0;
}
.redo {
    width: 16px;
    height: 16px;
    background-image: url(""../images/redo.svg"");
    margin: 0 0 0 8px;
}")</CodeSnippetTabPage>
</CodeSnippetTabbed>
